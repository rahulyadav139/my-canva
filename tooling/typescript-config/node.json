{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "display": "Node",
  "compilerOptions": {
    "composite": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noPropertyAccessFromIndexSignature": false,
    "strictNullChecks": true
  }
}
